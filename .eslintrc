{
	"env": {
		"browser": true
	},
	"globals": {
		"sap": true,
		"jQuery": true
	},
	"rules": {
		"no-console": 2,
		"block-scoped-var": 1,
		"brace-style": [2, "1tbs", { "allowSingleLine": true }],
		"consistent-this": 2,
		"no-div-regex": 2,
		"no-floating-decimal": 2,
		"no-self-compare": 2,
		"no-mixed-spaces-and-tabs": [2, true],
		"no-nested-ternary": 2,
		"no-unused-vars": [2, {"vars":"all", "args":"none"}],
		"radix": 2,
		"keyword-spacing": 2,
		"space-unary-ops": 2,
		"wrap-iife": [2, "any"],
		"camelcase": 1,
		"consistent-return": 1,
		"max-nested-callbacks": [1, 3],
		"new-cap": 1,
		"no-extra-boolean-cast": 1,
		"no-lonely-if": 1,
		"no-new": 1,
		"no-new-wrappers": 1,
		"no-redeclare": 1,
		"no-unused-expressions": 1,
		"no-use-before-define": [1, "nofunc"],
		"no-warning-comments": 1,
		"strict": 1,
		"valid-jsdoc": [1, {
			"requireReturn": false
		}],
		"default-case": 1,

		"dot-notation": 0,
		"eol-last": 0,
		"eqeqeq": 0,
		"no-trailing-spaces": 0,
		"no-underscore-dangle": 0,
		"comma-dangle": 2,
		"curly": 2,
		"new-parens": 2,
		"no-alert": 1,
		"no-array-constructor": 0,
		"no-caller": 2,
		"no-catch-shadow": 1,
		"no-cond-assign": 1,
		"no-constant-condition": 2,
		"no-control-regex": 1,
		"no-debugger": 2,
		"no-delete-var": 1,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty": 2,
		"no-empty-character-class": 2,
		"no-eval": 2,
		"no-ex-assign": 2,
		"no-extend-native": 1,
		"no-extra-bind": 1,
		"no-extra-semi": 2,
		"no-fallthrough": 1,
		"no-func-assign": 2,
		"no-implied-eval": 1,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 1,
		"no-label-var": 1,
		"no-labels": 1,
		"no-lone-blocks": 1,
		"no-loop-func": 1,
		"no-multi-str": 1,
		"no-native-reassign": 2,
		"no-negated-in-lhs": 2,
		"no-new-func": 2,
		"no-new-object": 0,
		"no-obj-calls": 2,
		"no-octal": 1,
		"no-octal-escape": 1,
		"no-process-exit": 1,
		"no-proto": 1,
		"no-regex-spaces": 1,
		"no-return-assign": 1,
		"no-script-url": 1,
		"no-sequences": 1,
		"no-shadow": 1,
		"no-shadow-restricted-names": 1,
		"no-spaced-func": 0,
		"no-sparse-arrays": 2,
		"no-throw-literal": 1,
		"no-undef": 2,
		"no-undef-init": 1,
		"no-unreachable": 2,
		"no-void": 1,
		"no-with": 2,
		"quotes": ["error", "double"],
		"semi": 2,
		"use-isnan": 2,
		"valid-typeof": 1,
		"yoda": 1,
		"no-unsafe-finally": 0
		
	}
}
